    /* Header style*/
.header {
  background-color:#FFFFFF ;
  color: #76D7C4;
  font-family: Trebuchet MS;
  text-align: right;
  padding-top:20px;
  padding-left: 20px;
  padding-right: 20px;
  min-height: 150px;
} 

/* Header card style*/
.headercard{
  background-color:#92b0e1 ;
  /* background-image: url("data/buildings-336574_960_720-1.jpg"); */
  color: #010a08;
  font-family: Trebuchet MS;
  display: grid;
  grid-template-columns: 150px 750px 200px;
  /* border-radius: 25px; */
  min-height: 100px;
  padding-left: 20px;
  padding-right: 20px;
}
    
    body{
        font-family: Verdana
    }
    
    .area_barchart{
        float: left;
        background-color: #f8b6b6;
        width: 500px;
        height: 500px;
        text-align: center;
        font-size: 20px;
        font-family: Verdana;
     }
     .mode_piechart{
        padding: 10px 10px 10px 70px;
        float: left;
        width: 300px;
        height: 300px;
        background-color: #ffffff;
        text-align: left;
        font-size: 20px;
        font-family: Verdana;
     }
     .status_piechart{
         padding: 10px 70px 10px 10px;
        float: right;
        width: 300px;
        height: 300px;
        background-color: #ffffff;
        text-align: left;
        font-size: 20px;
        font-family: Verdana;
     }
     .day_lollipop{
        float: left;
        width: 500px;
        height: 500px;
        background-color: #ffffff;
        text-align: center;
        font-size: 20px;
        font-family: Verdana;
     }

     .agency_barchart{
        float: left;
        width: 1000px;
        height: 500px;
        background-color: #ffffff;
        text-align: center;
        font-size: 20px;
        font-family: Verdana;

     }

     .page {
          max-width: 1000px;
          margin: 80px auto;
          background-color: #ffffff;
          height: 2300px;
          box-shadow: 6px 10px 28px 0px rgba(0,0,0,0.4);
        }
    .blankrow{
        background-color: #e9e8b0;
        }
        
        
    .dataDescription .drop {
        padding-right: 100px;
        float: right;
        width:230px;
        font-size: 12px;
        font-family: Verdana;
    }
    
    #dropdown{
        
        width:150px;
        height:30px;
        font-size: 12px;
        font-family: Verdana;
        
    }

    .topnav {

    /* padding: 10px 20px 10px 20px; */
          overflow: hidden;
          background-color:  #34495e;
          padding-left: 20px;
          padding-right: 20px;
        }

  .dataDescription{
    /* padding: 10px 20px 10px 20px; */
        float: right;
        font-size: 14px;
        font-family: Verdana;
        padding-left: 20px;
        padding-right: 20px;
  }

        .topnav a {
          background-color: #34495e;
          background-clip: padding-box;
          float: right;
          color: #ffffff ;
          text-align: center;
          /* padding: 0px 16px; */
          /* text-decoration: none; */
          font-size: 17px;
          padding-left: 20px;
          padding-right: 20px;
          position: relative;
        }

        .topnav a:hover {
          background-color: #ddd;
          color: black;
          padding-left: 20px;
          padding-right: 20px;
          background-clip: padding-box;
        }

        .topnav a.active {
          background-color: #34495e;
          color: #fad7a0;
          padding-left: 20px;
          padding-right: 20px;
          background-clip: padding-box;
        }